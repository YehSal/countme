
<%= form_for @voter do |i| %>
<%= i.label 'First Name' %>
<%= i.text_field :name_f%><br />

<%= i.label 'Last Name' %>
<%= i.text_field :name_l %><br />

  <%= i.label :address %>
  <%= i.text_field :address %><br />

  <%= i.label :city %>
  <%= i.text_field :city %><br />

  <%= i.label :state %>
  <%= i.text_field :state %><br />

  <%= i.label :zip  %>
  <%= i.text_field :zip %><br />

  <%= i.submit %>
<% end %>

<%= link_to "Delete this Entry", voter_path(@voter.id), method: 'delete'%>
