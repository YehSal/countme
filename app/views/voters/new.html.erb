<h1>Voter#new</h1>
<p>Find me in app/views/voter/new.html.erb</p>

<%= form_for @voter_form do |i| %>
  <%= i.label 'First Name' %>
  <%= i.text_field :name_f%><br />

  <%= i.label 'Last Name' %>
  <%= i.text_field :name_l %><br />

  <%= i.label :address %>
  <%= i.text_field :address %><br />

  <%= i.label :city %>
  <%= i.text_field :city %><br />

  <%= i.label :state %>
  <%= i.text_field :state %><br />

  <%= i.label :zip  %>
  <%= i.text_field :zip %><br />

  <%= i.submit %>
<% end %>
